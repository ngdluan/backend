generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id       String   @id @default(uuid())
  userName String?  @unique
  email    String   @unique
  hash     String
  details  Detail[]
}

model Detail {
  id     String  @id @default(uuid())
  User   User?   @relation(fields: [userId], references: [id])
  userId String?
  key    String?
  value  String?
}
